msgid "plugins.gateways.swordserver.servicedocument"
msgstr ""
"<p>Des dépôts SWORD peuvent être effectués sur ce document de service en "
"utilisant l'URL du document de service : <pre>{$serviceDocumentUrl}</pre></p>"
"\n"
"<p>Des collections spécifiques peuvent être traitées en utilisant "
"l'identifiant de la rubrique. Par exemple, pour l'identifiant de la rubrique "
"1 : <pre>{$exampleDepositPointUrl}</pre></p>"

msgid "plugins.gateways.swordserver.displayName"
msgstr "Module de serveur SWORD"

msgid "plugins.gateways.swordserver.description"
msgstr "Une API pour accepter les dépôts SWORD"
